<%- include('../partials/html-head') %>
  <%- include('../partials/nav') %>
  <% if (results) {%> 
    <main>
        <h1>Search Results</h1>
          <%results.forEach(anime => {%>
            <h2><%= anime.title%></h2>
            <img src="<%= anime.image_url%>">
            <h2><%= anime.synopsis%></h2>
            <h2><%= anime.type%></h2>
            <h2><%= anime.episodes%></h2>
            <h2><%= anime.rated%></h2>
          <% }) %>
        <% } %>
    </main>
    
    <%- include('../partials/footer') %>